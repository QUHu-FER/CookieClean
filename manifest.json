{
  "manifest_version": 3,
  "name": "CookieClean",
  "version": "1.0",
  "description": "Clean browser cookies with one click",
  "permissions": ["cookies", "tabs", "storage", "browsingData"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "32": "icons/icon32.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "32": "icons/icon32.png",
    "128": "icons/icon128.png"
  }
}
